{
  "name": "style-resources-loader",
  "version": "1.0.0",
  "description": "node6 compatible version of style-resources-loader",
  "engines": {
    "node": ">=6.10"
  },
  "main": "lib/index.js",
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phlogisticfugu/style-resources-loader-node6.git"
  },
  "author": "Brad Ito <phlogisticfugu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phlogisticfugu/style-resources-loader-node6/issues"
  },
  "homepage": "https://github.com/phlogisticfugu/style-resources-loader-node6#readme",
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true,
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "helpers": true,
          "polyfill": true,
          "regenerator": true,
          "mooduleName": "babel-runtime"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "shelljs": "^0.8.1",
    "style-resources-loader": "1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
